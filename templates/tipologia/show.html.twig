{% extends 'base.html.twig' %}

{% block title %}Detalle de Tipología{% endblock %}

{% block body %}
    <h1 class="mb-4">Detalle de Tipología</h1>

    <table class="table table-bordered">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipologium.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ tipologium.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_tipologia_index') }}" class="btn btn-secondary mt-3">Volver a la lista</a>
    <a href="{{ path('app_tipologia_edit', {'id': tipologium.id}) }}" class="btn btn-primary mt-3">Editar</a>

    {{ include('tipologia/_delete_form.html.twig') }}
{% endblock %}

